{% if CSS_POOL is defined %}
    {% for cssFile in CSS_POOL %}
        <link href="{{ cssFile }}" rel="stylesheet" />
    {% endfor %}
{% endif %}
{% if JS_VARS is defined and JS_VARS|length > 0 %}
    <script>
    {% for jsVarName, jsVarValue in JS_VARS  %}
        var {{ jsVarName }} = "{{ jsVarValue }}";
    {% endfor %}
    </script>
{% endif %}
{% if JS_VARS_RAW is defined %}
    <script>
    {% for jsVarName, jsVarValue in JS_VARS_RAW  %}
        var {{ jsVarName }} = {{ jsVarValue|raw }};
    {% endfor %}
    </script>
{% endif %}
{% if JS_POOL is defined %}
    {% for jsFile in JS_POOL %}
        <script src="{{ jsFile }}"></script>
    {% endfor %}
{% endif %}
{{ content|raw }}
{% if JS_END_POOL is defined %}
    {% for jsFile in JS_END_POOL %}
        <script src="{{ jsFile }}"></script>
    {% endfor %}
{% endif %}
