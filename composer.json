{
    "name": "nextdom/core",
    "description": "Software for home automation",
    "require": {
        "christian-riesen/base32": "1.3.*",
        "doctrine/cache": "1.6.*",
        "guzzle/guzzle": "3.9.*",
        "guzzlehttp/guzzle": "6.3.*",
        "guzzlehttp/promises": "1.3.*",
        "guzzlehttp/psr7": "1.4.*",
        "knplabs/github-api": "1.7.*",
        "league/flysystem": "1.0.*",
        "league/oauth2-client": "2.3.*",
        "league/flysystem-webdav": "1.0.*",
        "monolog/monolog": "1.23.*",
        "mtdowling/cron-expression": "1.2.*",
        "paragonie/random_compat": "2.0.*",
        "pragmarx/google2fa": "1.0.*",
        "psr/http-message": "1.0.*",
        "psr/log": "1.0.*",
        "sabre/dav": "3.2.*",
        "sabre/event": "3.0.*",
        "sabre/http": "4.2.*",
        "sabre/uri": "1.2.*",
        "sabre/vobject": "4.1.*",
        "sabre/xml": "1.5.*",
        "symfony/event-dispatcher": "2.8.*",
        "symfony/expression-language": "2.8.*",
        "symfony/polyfill-php56": "1.8.*",
        "symfony/polyfill-util": "1.8.*",
        "touki/ftp": "1.2.*",
        "symfony/translation": "^3.4"
        
    },
    "license": "GPL",
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "7.0"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/debug": "^3.4"
    },
    "authors": [
        {
            "name": "nextdom",
            "email": "nextdom@nextdom.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "NextDom\\": [
                "core/class",
                "core/src",
                "core/ajax/"
            ]
        }
    }
}
